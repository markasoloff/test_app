<%= form_tag "/views/events/#{@event['id']}", method: :patch do %>
  <div>
    Event Title: <input type="text" name="title" value="<%= @event['title'] %>" />
  </div>
  <div>
    Start Date: <input type="text" name="start" value="<%= @event['start'] %>" />
  </div>
  <div>
    End Date: <input type="text" name="end" value="<%= @event['end'] %>" />
  </div>
  <input type="submit" name="" value="Update Event" />
<% end %>
